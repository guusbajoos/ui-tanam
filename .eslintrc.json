{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "eslint:recommended", "next", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"no-console": ["warn"],
		"prettier/prettier": ["error"],
		"react/jsx-sort-props": ["warn", { "ignoreCase": true }],
		"semi": "error",
		"react/prop-types": 0
	}
}
